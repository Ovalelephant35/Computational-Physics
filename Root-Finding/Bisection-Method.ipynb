   "source": [
    "import numpy as np\n",
    "from scipy.optimize import bisect\n",
    "\n",
    "def f(x):\n",
    "    return x**2 - 4\n",
    "\n",
    "a = -1000000.0\n",
    "b = 0\n",
    "\n",
    "root = bisect(f, a, b)\n",
    "\n",
    "print(f\"Root found at x = {root}\")"
   ]
